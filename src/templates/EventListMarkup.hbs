
<ul class="event-list">
  {{#each this}}
    <li class="event-item" id={{id}}>
            <img class="event-item__img" src={{images.0.url}} alt={{name}} width="200" height="120" loading="lazy" />
            <div class="event-item__info">
              <p class="info-item">{{name}}</p>
              <p class="info-item">{{dates.start.localDate}}</p>
              <p class="info-item">{{_embedded.venues.0.city.name}},{{_embedded.venues.0.address.line1}}</p>
             
            </div>
    </li>
  {{/each}}
</ul>
