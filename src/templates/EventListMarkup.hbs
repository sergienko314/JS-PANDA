
 {{#each this}}
<ul>
    <li class="gallery-item" id={{id}}>
            <img class="gallery-item__img" src={{images.0.url}} alt={{name}} width="200" height="120" loading="lazy" />
            <div class="info">


<ul class="event-list">
  {{#each this}}
    <li class="event-item" id={{id}}>
            
            <div class="event-item__info">
              <img class="event-item__img" src={{images.0.url}} alt={{name}} loading="lazy" />
              <p class="info-item__title">{{name}}</p>
              <p class="info-item__date">{{dates.start.localDate}}</p>
              <p class="info-item__location">{{_embedded.venues.0.city.name}},{{_embedded.venues.0.address.line1}}</p>
             
            </div>
    </li>
  {{/each}}
</ul>


{{/each}} 



